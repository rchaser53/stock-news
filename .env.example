# Gemini
GEMINI_API_KEY=
# rag-playground に合わせた既定モデル名
GEMINI_CHAT_MODEL=gemini-3-pro-preview
# このプロジェクト固有（未指定時は GEMINI_CHAT_MODEL / GEMINI_MODEL を利用）
GEMINI_MODEL_NEWS=
GEMINI_MODEL_SUMMARY=

# Gemini REST API version (v3 は v1beta のエイリアス扱い)
GEMINI_API_VERSION=v3

# ニュース取得時に googleSearch tool を使う（環境により使えない場合は自動でオフにフォールバック）
GEMINI_ENABLE_GOOGLE_SEARCH=true

# デバッグ: Geminiの空応答などで body を出す
GEMINI_DEBUG=false

# eval JSONL
EVAL_OUTPUT_DIR=eval_runs
# EVAL_RUN_FILE=eval_runs/my_run.jsonl
